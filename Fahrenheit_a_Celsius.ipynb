   "source": [
    "def convertidor(s):\n",
    "    f = float(s)\n",
    "\n",
    "#Formula para aplicar\n",
    "    c = (f - 32) * 5/9\n",
    "    return c\n",
    "celsius = input(\"Ingrese valores en Fahrenheit : \")\n",
    "convertidor(celsius)    "
   ]
